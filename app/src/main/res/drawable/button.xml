<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Pressed state -->
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#C51162"/> <!-- Change color to your preference -->
            <corners android:radius="04dp"/> <!-- Change radius to your preference -->
        </shape>
    </item>

    <!-- Default state -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#C51162"/> <!-- Change color to your preference -->
            <corners android:radius="04dp"/> <!-- Change radius to your preference -->
        </shape>
    </item>

</selector>
